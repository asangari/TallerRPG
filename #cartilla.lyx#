\end_layout

\begin_deeper
\begin_layout Exercise
\begin_inset CommandInset label
LatexCommand label
name "rectángulo-circunferencia"

\end_inset

 Muestre que 
\shape italic
un triángulo es rectángulo si y sólo si uno de sus lados diámetro de la
 circunferencia que pasa por sus vértices
\shape default
.
 
\end_layout

\end_deeper
\end_deeper
\end_deeper
\begin_layout Theorem
\begin_inset CommandInset label
LatexCommand label
name "thm:Ang-Ins-Suplem"

\end_inset

Sea una circunferencia 
\begin_inset Formula $\mathcal{C}$
\end_inset

 de centro 
\begin_inset Formula $O$
\end_inset

 y una cuerda 
\begin_inset Formula $AB$
\end_inset

.
 Si el ángulo inscrito 
\begin_inset Formula $\beta$
\end_inset

 subtiende a 
\begin_inset Formula $AB$
\end_inset

 desde el otro lado de 
\begin_inset Formula $O$
\end_inset

 con respecto a 
\begin_inset Formula $AB$
\end_inset

, 
\begin_inset Formula $\beta=\pi-\alpha$
\end_inset

, donde 
\begin_inset Formula $\alpha$
\end_inset

 es la mitad del ángulo central.
\end_layout

\begin_layout Exercise
Demuestre el Teorema 
\begin_inset CommandInset ref
LatexCommand ref
reference "thm:Ang-Ins-Suplem"

\end_inset

.
\end_layout

\begin_deeper
\begin_layout Exercise
\begin_inset CommandInset label
LatexCommand label
name "InsCongruentes"

\end_inset

A partir de los teoremas 
\begin_inset CommandInset ref
LatexCommand ref
reference "thm:Ang-Insc"

\end_inset

 y 
\begin_inset CommandInset ref
LatexCommand ref
reference "thm:Ang-Ins-Suplem"

\end_inset

, justifique que 
\shape italic
los ángulos inscritos en el mismo arco son congruentes
\shape default
.
\end_layout

\begin_deeper
\begin_layout Exercise
Justifique que 
\shape italic
las rectas paralelas determinan arcos congruentes sobre un círculo
\shape default
.
\end_layout

\begin_deeper
\begin_layout Exercise
Demuestre que 
\shape italic
si un cuadrilátero está inscrito en una circunferencia sus ángulos opuestos
 son suplementarios
\shape default
.
 
\end_layout

